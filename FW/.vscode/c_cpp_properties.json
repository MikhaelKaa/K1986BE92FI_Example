{
    "configurations": [
        {
            "name": "arm_c_cpp_properties",
            "compilerPath": "C:/arm_tool/gcc/bin/arm-none-eabi-gcc.exe",
            "includePath": [
                "C:/arm_tool/gcc/arm-none-eabi/include",
                "C:/arm_tool/gcc/arm-none-eabi/include/sys/",
                "C:/arm_tool/gcc/lib/**",
                "${workspaceRoot}\\platform\\**",
                "${workspaceFolder}/platform/SPL/MDR32FxQI",
                "${workspaceFolder}/platform/SPL/MDR32FxQI/inc",
                "${workspaceFolder}/app/retarget",
                "${workspaceFolder}/app/u_read_line",
                "${workspaceFolder}/app/mem",
                "${workspaceFolder}/app/micros",
                "${workspaceFolder}/app/coremark"
            ],
            "defines": [
                "HSE_Value=8000000",
                "USE_K1986VE92xI",
                "ITERATIONS=4096",
                "CLOCKS_PER_SEC=1000",
                "COREMARK_TIME=millis"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "windows-gcc-arm"
        }
    ],
    "version": 4
}